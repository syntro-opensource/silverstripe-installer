{
    "name": "syntro/silverstripe-ssto",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer for Syntro Projects",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.6.x-dev",
        "silverstripe/login-forms": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5"
    },
    "extra": {
        "expose": [
            "themes/orbit/dist/js",
            "themes/orbit/dist/style",
            "themes/orbit/dist/favicons",
            "themes/orbit/files"
        ],
        "project-files": [
            "app/_config/*",
            "themes/orbit/*"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ],
        "resources-dir": "_resources"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
